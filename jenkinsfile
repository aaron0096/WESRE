pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
        stage('Hello2') {
            steps {
                echo 'Hello World no.2'
            }
        }
        stage('Python 3 test run') {
            steps {
                python3 ./FizzBuzz.py
            }
        }   
    }   
}
